{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "plugin:vue/vue3-recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "no-console": 1
    }
}